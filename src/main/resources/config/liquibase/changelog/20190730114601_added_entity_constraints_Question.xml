<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Question.
    -->
    <changeSet id="20190730114601-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="exam_id"
                                 baseTableName="question"
                                 constraintName="fk_question_exam_id"
                                 referencedColumnNames="id"
                                 referencedTableName="exam"/>

        <addForeignKeyConstraint baseColumnNames="question_bank_id"
                                 baseTableName="question"
                                 constraintName="fk_question_question_bank_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question_bank"/>

    </changeSet>
</databaseChangeLog>
